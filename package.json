{
  "name": "nuxt3-awesome-starter",
  "version": "0.1.0",
  "description": "a Nuxt 3 starter template or boilerplate with a lot of useful features. Nuxt3 + Tailwindcss 3",
  "repository": {
    "type": "git",
    "url": "git://github.com/viandwi24/nuxt3-awesome-starter"
  },
  "author": "viandwi24",
  "license": "MIT",
  "scripts": {
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "serve": "serve dist/",
    "postinstall": "husky install",
    "lint": "eslint --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
    "lintfix": "eslint --fix --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
    "prepare": "husky install",
    "clean": "rm -rf .nuxt dist .output",
    "lint-staged": "lint-staged",
    "nuxt:build": "nuxt build",
    "nuxt:dev": "nuxt dev",
    "nuxt:start": "node .output/server/index.mjs",
    "start": "sh ./others/scripts/launch.sh",
    "dev": "sh ./others/scripts/launch.sh dev",
    "proxy:log": "docker logs -f filbet-proxy-service",
    "stop": "sh ./others/scripts/stop.sh",
    "launch": "sh ./others/scripts/launch.sh docker",
    "generate:assetkeys": "sh ./others/scripts/gen.asset.keys.sh"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@element-plus/nuxt": "^1.0.4",
    "@headlessui/vue": "^1.7.8",
    "@iconify/json": "^2.1.135",
    "@intlify/nuxt3": "^0.2.4",
    "@nuxt/test-utils-edge": "3.1.1-rc.0-27911047.7d812db",
    "@nuxtjs/eslint-config": "^12.0.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@pinia/nuxt": "^0.4.7",
    "@vueuse/core": "^9.11.1",
    "@vueuse/nuxt": "^9.11.1",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "js-yaml": "^4.1.0",
    "lint-staged": "^13.0.3",
    "nuxt": "3.1.1",
    "nuxt-windicss": "^2.6.0",
    "postcss": "8.4.14",
    "postcss-loader": "^7.0.0",
    "prettier": "^2.7.1",
    "sass": "1.53.0",
    "sass-loader": "^13.2.0",
    "serve": "^13.0.2",
    "three": "^0.143.0",
    "translate": "^1.4.1",
    "typescript": "^4.9.4",
    "unplugin-element-plus": "^0.7.0",
    "unplugin-icons": "^0.14.14",
    "unplugin-vue-components": "^0.22.9",
    "vite": ">=2.9.0 <3.0.0 || >=3.0.0-0 <3.0.0",
    "vitest": "^0.28.3",
    "vue": "^3.2.41",
    "vue-tsc": "^1.0.9",
    "webpack": ">=5.0.0 <6.0.0"
  },
  "lint-staged": {
    "**/*.{js,ts,vue,html}": [
      "pnpm lintfix"
    ]
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^0.10.0",
    "@types/blueimp-md5": "^2.18.0",
    "@types/dplayer": "^1.25.2",
    "@types/form-data": "^2.5.0",
    "@types/node": "^18.15.11",
    "@types/qrcode": "^1.5.0",
    "axios": "^1.3.4",
    "blueimp-md5": "^2.19.0",
    "crypto-js": "^4.1.1",
    "dplayer": "^1.27.1",
    "element-plus": "^2.3.1",
    "form-data": "^4.0.0",
    "js-md5": "^0.7.3",
    "node-fetch-native": "^1.0.2",
    "pinia": "^2.0.33",
    "pnpm": "^8.1.0",
    "qrcode": "^1.5.1",
    "vue-fullscreen": "^2.6.1"
  }
}
